{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.11.2",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "js/all.min.js",
        "sprites/*",
        "webfonts/*"
      ]
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    },
    {
      "library": "jquery.serializeJSON@2.9.0",
      "destination": "wwwroot/lib/jquery-serializeJSON/"
    },
    {
      "library": "bootstrap-table@1.16.0",
      "destination": "wwwroot/lib/bootstrap-table/",
      "files": [
        "bootstrap-table.min.css",
        "bootstrap-table.min.js",
        "extensions/filter-control/bootstrap-table-filter-control.min.css",
        "extensions/filter-control/bootstrap-table-filter-control.min.js",
        "extensions/fixed-columns/bootstrap-table-fixed-columns.min.css",
        "extensions/fixed-columns/bootstrap-table-fixed-columns.min.js"
      ]
    },
    {
      "library": "Sortable@1.10.2",
      "destination": "wwwroot/lib/sortable"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.5.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "library": "bootstrap-select@1.13.17",
      "destination": "wwwroot/lib/bootstrap-select/",
      "files": [
        "js/bootstrap-select.min.js.map",
        "css/bootstrap-select.css",
        "css/bootstrap-select.css.map",
        "css/bootstrap-select.min.css",
        "js/bootstrap-select.js",
        "js/bootstrap-select.js.map",
        "js/bootstrap-select.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-load-json@1.3.3",
      "destination": "wwwroot/lib/jquery-load-json/",
      "files": [
        "dist/jquery.loadJSON.min.js"
      ]
    },
    {
      "library": "bootstrap-fileinput@5.0.9",
      "destination": "wwwroot/lib/bootstrap-fileinput/",
      "files": [
        "js/fileinput.min.js",
        "css/fileinput.min.css",
        "img/loading-sm.gif",
        "img/loading.gif"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/additional-methods.min.js",
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    }
  ]
}